# Path
# User specific environment
DESIRED_USER_PATH="$HOME/.local/bin:$HOME/bin:.:$HOME/opt/eines-sisa/bin:$HOME/opt/flutter-sdk/flutter/bin:$HOME/.config/emacs/bin"
if ! [[ "$PATH" =~ "$DESIRED_USER_PATH:" ]]
then
    PATH="$DESIRED_USER_PATH:$PATH"
fi
export PATH

# Terminal line
export PS1='\[\033[01;32m\]\u@\h\[\033[01;34m\] \w \n=>\[\033[00m\] '

# Language and encoding
export LANG="en_US.UTF-8"
